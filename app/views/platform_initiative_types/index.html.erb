<h1>Listing platform_initiative_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Platform</th>
      <th>Endpoint</th>
      <th>Schema</th>
      <th>Field mapping</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @platform_initiative_types.each do |platform_initiative_type| %>
      <tr>
        <td><%= platform_initiative_type.name %></td>
        <td><%= platform_initiative_type.platform %></td>
        <td><%= platform_initiative_type.endpoint %></td>
        <td><%= platform_initiative_type.schema %></td>
        <td><%= platform_initiative_type.field_mapping %></td>
        <td><%= link_to 'Show', platform_initiative_type %></td>
        <td><%= link_to 'Edit', edit_platform_initiative_type_path(platform_initiative_type) %></td>
        <td><%= link_to 'Destroy', platform_initiative_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Platform initiative type', new_platform_initiative_type_path %>
